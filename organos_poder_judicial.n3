# Disjoint classes
# ================

derecho:OrganoJudicialEspanol a owl:AllDisjointClasses;
    owl:members  ( derecho:AudienciaNacional derecho:AudienciaProvincial
                 derecho:JuzgadoCentralInstruccion derecho:JuzgadoCentralMenores
                 derecho:JuzgadoCentralVigilanciaPenitenciaria derecho:JuzgadoContenciosoAdministrativo
                 derecho:JuzgadoInstruccion derecho:JuzgadoMenores derecho:JuzgadoMercantil
                 derecho:JuzgadoPaz derecho:JuzgadoPenal derecho:JuzgadoPrimeraInstancia
                 derecho:JuzgadoPrimeraInstanciaInstruccion
                 derecho:JuzgadoSocial derecho:JuzgadoVigilanciaPenitenciaria
                 derecho:JuzgadoViolenciaMujer derecho:TribunalSuperiorJusticia derecho:TribunalSupremo ) .

derecho:OrganoJudicialEspanol owl:oneOf
    ( derecho:AudienciaNacional derecho:AudienciaProvincial
    derecho:JuzgadoCentralInstruccion derecho:JuzgadoCentralMenores
    derecho:JuzgadoCentralVigilanciaPenitenciaria derecho:JuzgadoContenciosoAdministrativo
    derecho:JuzgadoInstruccion derecho:JuzgadoMenores derecho:JuzgadoMercantil
    derecho:JuzgadoPaz derecho:JuzgadoPenal derecho:JuzgadoPrimeraInstancia
    derecho:JuzgadoPrimeraInstanciaInstruccion
    derecho:JuzgadoSocial derecho:JuzgadoVigilanciaPenitenciaria
    derecho:JuzgadoViolenciaMujer derecho:TribunalSuperiorJusticia derecho:TribunalSupremo ) .

